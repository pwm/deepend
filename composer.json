{
    "name": "pwm/deepend",
    "description": "A simple library for scheduling dependent task execution",
    "type": "library",
    "keywords": ["scheduled-tasks", "topological-sort", "graph-algorithms"],
    "homepage": "https://github.com/pwm/deepend",
    "license": "MIT",
    "authors": [
        {
            "name": "Zsolt Szende",
            "email": "zs@szende.me"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "phpstan/phpstan": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "Pwm\\DeepEnd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pwm\\DeepEnd\\": "tests/unit/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse -l 7 src"
    }
}
